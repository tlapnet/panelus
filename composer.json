{
  "name": "tlapnet/panelus",
  "description": "Panels",
  "keywords": [
    "nette",
    "panels",
    "widgets"
  ],
  "type": "library",
  "license": "MPL-2.0",
  "homepage": "https://github.com/tlapnet/panelus",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Marek Bartoš",
      "homepage": "https://github.com/mabar"
    }
  ],
  "require": {
    "php": "^7.2.0",
    "contributte/utils": "^0.4.0",
    "nette/application": "~3.0.0",
    "nette/di": "~3.0.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "ninjify/qa": "^0.9.0",
    "phpstan/extension-installer": "^1.0.0",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-shim": "^0.11.0",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Tlapnet\\Panelus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Tlapnet\\Panelus\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
